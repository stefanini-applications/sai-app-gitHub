<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<RootNamespace>Stefanini.Innovations.SAIAPP.GitHub</RootNamespace>
	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<PackageId>Innovations.SAIAPP.GitHub</PackageId>
	<Company>Stefanini</Company>
	<Authors>Stefanini Innovations Team</Authors>
	<RepositoryType>git</RepositoryType>
	<RepositoryUrl>https://github.com/stefanini-applications/sai-app-gitHub.git</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Octokit" Version="14.0.0" />
  </ItemGroup>

</Project>
